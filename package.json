  "main": "./src/index.ts",
  "repository": "https://github.com/osawa-koki/simple-pythagoras-tree.ts.git",
  "author": "osawa-koki <koki@osawa.dev>",
  "license": "Apache-2.0",
  "private": false,
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
